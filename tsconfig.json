{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "useDefineForClassFields": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // "composite": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "$": [
        "packages/*"
      ],
      "$/*": [
        "packages/*"
      ],
      "@": [
        "src/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "jsxImportSource": "vue",
    "types": [
      "chriszj/packages/components"
    ]
  },
  "include": [
    "packages/**/*.ts",
    "packages/**/*.d.ts",
    "packages/**/*.tsx",
    "packages/**/*.vue",
    "packages/*.d.ts",
    "packages/Tform/comp/form-itemTsx.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}